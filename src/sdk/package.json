{
  "name": "SteamClientHomebrewSDK",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "lerna run test",
    "build": "lerna run build",
    "docs": "typedoc"
  },
  "devDependencies": {
    "lerna": "^9.0.3",
    "typedoc": "^0.28.15",
    "typedoc-plugin-frontmatter": "^1.3.1",
    "typedoc-plugin-markdown": "^4.9.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    },
    "updateConfig": {
      "ignoreDependencies": [
        "react",
        "react-dom"
      ]
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "electron",
      "nx"
    ]
  }
}
