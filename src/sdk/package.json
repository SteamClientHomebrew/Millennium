{
  "name": "@steambrew/docs",
  "version": "1.1.1",
  "description": "Documentation for the SteamBrew SDK.",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "docs"
  ],
  "scripts": {
    "test": "lerna run test",
    "build": "lerna run build",
    "docs": "typedoc",
    "prepublish": "npm run build && npm run docs"
  },
  "devDependencies": {
    "lerna": "^9.0.3",
    "typedoc": "^0.28.15",
    "typedoc-plugin-frontmatter": "^1.3.1",
    "typedoc-plugin-markdown": "^4.9.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    },
    "updateConfig": {
      "ignoreDependencies": [
        "react",
        "react-dom"
      ]
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "electron",
      "nx"
    ]
  }
}
